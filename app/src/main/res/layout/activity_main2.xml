<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayout2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <RelativeLayout
                android:id="@+id/relativeLayout3"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@drawable/shape"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <TextView
                    android:id="@+id/textView76"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView22"
                    android:layout_marginLeft="167dp"
                    android:layout_toRightOf="@+id/total" />

                <Button
                    android:id="@+id/clear"
                    android:layout_width="130dp"
                    android:layout_height="45dp"
                    android:background="@drawable/background"
                    android:text="Clear"
                    android:layout_marginBottom="20dp"
                    android:textColor="@android:color/black"

                    android:textStyle="bold"
                    android:layout_alignParentBottom="true"
                    android:layout_alignLeft="@+id/textView22"
                    android:layout_alignStart="@+id/textView22"
                    android:layout_alignTop="@+id/back" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0 Hours/Day"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#000000"

                    android:layout_alignBaseline="@+id/textView1"
                    android:layout_alignBottom="@+id/textView1"
                    android:layout_toRightOf="@+id/textView1"
                    android:layout_toEndOf="@+id/textView1" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Состояние :"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#000000"

                    android:typeface="sans"
                    android:layout_marginBottom="26dp"
                    android:layout_above="@+id/textView4"
                    android:layout_alignLeft="@+id/textView4"
                    android:layout_alignStart="@+id/textView4" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="Общее время : "
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#000000"

                    android:typeface="sans"
                    android:layout_above="@+id/clear"
                    android:layout_alignLeft="@+id/textView1"
                    android:layout_alignStart="@+id/textView1"
                    android:layout_marginBottom="151dp" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="20dp"
                    android:paddingBottom="10dp"
                    android:text="Статистика : "
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#000000"

                    android:typeface="sans"
                    android:layout_marginStart="15dp"
                    android:layout_below="@+id/total"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <Button
                    android:id="@+id/back"
                    android:layout_width="130dp"
                    android:layout_height="45dp"
                    android:background="@drawable/background"
                    android:text="Back"
                    android:layout_marginBottom="20dp"
                    android:textColor="@android:color/black"

                    android:textStyle="bold"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true" />

                <TextView
                    android:id="@+id/total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="00 Hours"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#000000"

                    android:typeface="sans"
                    android:layout_alignBaseline="@+id/textView4"
                    android:layout_alignBottom="@+id/textView4"
                    android:layout_toRightOf="@+id/textView4"
                    android:layout_toEndOf="@+id/textView4" />

                <TextView
                    android:id="@+id/state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Low"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#32CD32"

                    android:typeface="sans"
                    android:layout_below="@+id/imageView2"
                    android:layout_toRightOf="@+id/textView1"
                    android:layout_toEndOf="@+id/textView1" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    ads:srcCompat="@drawable/facebook"
                    android:id="@+id/imageView2"
                    android:layout_marginBottom="52dp"
                    android:layout_above="@+id/textView22"
                    android:layout_alignRight="@+id/clear"
                    android:layout_alignEnd="@+id/clear" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    ads:srcCompat="@drawable/instagram"
                    android:id="@+id/imageView3"
                    android:layout_alignTop="@+id/imageView2"
                    android:layout_alignLeft="@+id/state"
                    android:layout_alignStart="@+id/state" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    ads:srcCompat="@drawable/twitter"
                    android:id="@+id/imageView4"
                    android:layout_alignTop="@+id/imageView3"
                    android:layout_toRightOf="@+id/imageView3"
                    android:layout_toEndOf="@+id/imageView3" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    ads:srcCompat="@drawable/rsz_vk_icon"
                    android:id="@+id/imageView9"
                    android:layout_alignTop="@+id/imageView2"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />


            </RelativeLayout>
        </RelativeLayout>



</RelativeLayout>